#include <Stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
	int score[3];
	int total=0;
	double avg;

	for (int i = 0; i < 3; i++) {
		scanf_s("%d", &score[i]);//이게 값을 넣는거

	}
	for (int j = 0; j < 3; j++) {
		total += score[j];//조건  

	}
	avg = total / 3.0;
	printf("총점:% d\n", total);//결과값 
	return 0;
}
